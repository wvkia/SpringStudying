<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helloWorldService" class="com.wvkia.tinyioc.bean.service.HelloWorldServiceImpl">
        <property name="text" value="hello world"/>
        <property name="outputService" ref="outputService"/>
    </bean>

    <bean id="outputService" class="com.wvkia.tinyioc.bean.service.OutputServiceImpl"/>

</beans>